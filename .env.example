# Kaspa V3 Foundry Network Configuration
# Copy this to .env and fill in your private key

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Etherscan API key for verification
ETHERSCAN_API_KEY=EtherscanApi

# Network configurations
KASPLEX_TESTNET_RPC_URL=https://rpc.kasplextest.xyz
KASPLEX_MAINNET_RPC_URL=https://rpc.kasplextest.xyz

# Chain IDs
KASPLEX_TESTNET_CHAIN_ID=167012
KASPLEX_MAINNET_CHAIN_ID=167012

# Explorer URLs for verification
KASPLEX_TESTNET_EXPLORER=https://explorer.testnet.kasplextest.xyz
KASPLEX_MAINNET_EXPLORER=https://explorer.testnet.kasplextest.xyz
